{
  "name": "PostgramX-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && npm run copy:i18n",
    "copy:i18n": "cp -r src/i18n dist/",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production nest start --watch",
    "start:production": "NODE_ENV=production node dist/main",
    "start:local": "NODE_ENV=local nest start --watch",
    "start:local-mock": "npm run seed:local-mock && npm run start:local",
    "start:stage": "NODE_ENV=stage nest start --watch",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "seed:local-mock": "NODE_ENV=local ts-node -r tsconfig-paths/register src/scripts/seed-local-mock.ts",
    "seed:stage-mock": "NODE_ENV=stage ts-node -r tsconfig-paths/register src/scripts/seed-local-mock.ts",
    "seed:withdrawable-balance": "ts-node -r tsconfig-paths/register src/scripts/create-withdrawable-balance.ts",
    "cleanup:deals": "ts-node -r tsconfig-paths/register src/scripts/cleanup-deals-data.ts",
    "cleanup:deals:stage": "NODE_ENV=stage ts-node -r tsconfig-paths/register src/scripts/cleanup-deals-data.ts",
    "clean:local-mock": "NODE_ENV=local ts-node -r tsconfig-paths/register src/scripts/cleanup-local-mock.ts",
    "clean:stage-mock": "NODE_ENV=stage ts-node -r tsconfig-paths/register src/scripts/cleanup-local-mock.ts",
    "deploy:hot-wallet": "ts-node src/scripts/deploy-hot-wallet.ts",
    "telegram:publish-channel-message": "ts-node -r tsconfig-paths/register src/scripts/publish-channel-message.ts",
    "telegram:edit-channel-publication": "ts-node -r tsconfig-paths/register src/scripts/edit-channel-publication.ts",
    "clean:db:keep-users:local": "NODE_ENV=local ts-node -r tsconfig-paths/register src/scripts/cleanup-db-keep-users.ts",
    "clean:db:keep-users:stage": "NODE_ENV=stage ts-node -r tsconfig-paths/register src/scripts/cleanup-db-keep-users.ts",
    "sweep:completed-deals": "ts-node -r tsconfig-paths/register src/scripts/sweep-completed-deals-to-hot-wallet.ts",
    "mtproto:generate-session": "ts-node -r tsconfig-paths/register src/scripts/generate-mtproto-session.ts"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/nestjs": "^6.1.0",
    "@adminjs/themes": "^1.0.1",
    "@adminjs/typeorm": "^5.0.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.17",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.1.2",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@ton/crypto": "^3.3.0",
    "@ton/ton": "^16.1.0",
    "adminjs": "^7.8.17",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "express-formidable": "^1.2.0",
    "express-session": "^1.18.2",
    "firebase-admin": "^13.4.0",
    "fs": "^0.0.1-security",
    "grammy": "^1.36.3",
    "input": "^1.0.1",
    "nestjs-i18n": "^10.5.1",
    "nodemailer": "^7.0.9",
    "openai": "^6.6.0",
    "path": "^0.12.7",
    "pg": "^8.16.3",
    "puppeteer": "^24.26.0",
    "puppeteer-core": "^24.26.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "telegraf": "^4.16.3",
    "telegram": "^2.26.22",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@sparticuz/chromium": "^141.0.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.2",
    "@types/node": "^20.17.56",
    "@types/puppeteer": "^5.4.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
